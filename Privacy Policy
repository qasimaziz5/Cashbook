# Let's create a privacy.html file content with Qasim Aziz's details and app info.
privacy_html = """<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Privacy Policy - iCashPro</title>
  <style>
    body { font-family: Arial, sans-serif; line-height: 1.6; margin: 20px; padding: 0; background: #f9f9f9; color: #333; }
    h1, h2 { color: #004080; }
    a { color: #0066cc; text-decoration: none; }
    a:hover { text-decoration: underline; }
    .container { background: #fff; padding: 20px; border-radius: 8px; box-shadow: 0 2px 5px rgba(0,0,0,0.1); }
  </style>
</head>
<body>
  <div class="container">
    <h1>Privacy Policy for iCashPro</h1>
    <p><strong>Effective Date:</strong> October 2025</p>

    <p>Your privacy is important to us. iCashPro, developed by Qasim Aziz, does not collect, store, or share personal information. 
       All data you enter is saved locally on your device and is not transmitted to external servers.</p>

    <h2>Data Collection</h2>
    <p>We do not collect personal information such as names, emails, phone numbers, or financial details. 
       Any data you enter is stored only on your device.</p>

    <h2>Third-Party Services</h2>
    <p>This app does not use third-party analytics, ads, or tracking tools.</p>

    <h2>Children's Privacy</h2>
    <p>Our app is suitable for all ages and does not knowingly collect information from children.</p>

    <h2>Changes to this Policy</h2>
    <p>We may update our Privacy Policy from time to time. Any changes will be reflected on this page.</p>

    <h2>Contact Us</h2>
    <p>If you have any questions about this Privacy Policy, you may contact us at:  
       <a href="mailto:qasim.aziz@hotmail.com">qasim.aziz@hotmail.com</a></p>
  </div>
</body>
</html>
"""

# Save to a zip file for the user
import zipfile
import os

zip_path = "/mnt/data/privacy_policy.zip"

with zipfile.ZipFile(zip_path, 'w') as zf:
    zf.writestr("privacy.html", privacy_html)

zip_path
